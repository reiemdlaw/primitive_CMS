<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="pages")
 */
class Pages
{
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;
        /**
     * @ORM\Column(type="string", length=100)
     */

    private $linktext;
    
      /**
     * @ORM\Column(type="string", length=100)
     */
  
    private $url;
        /**
     * @ORM\Column(type="text")
     */

    private $headline;
        /**
     * @ORM\Column(type="text")
     */

    private $maintext;
}